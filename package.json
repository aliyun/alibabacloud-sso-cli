{
  "name": "@alicloud/sso-cli",
  "version": "1.1.0",
  "description": "Alibaba Cloud SSO command line interface",
  "bin": {
    "acs-sso": "bin/acs-sso.js"
  },
  "scripts": {
    "ci": "npm run lint && npm run cov && codecov",
    "test": "mocha -R spec test/**/*.test.js",
    "cov": "nyc -r html npm run test",
    "lint": "eslint --fix ./bin ./lib ./test ./commands",
    "build": "npm i --omit=dev && pkg ."
  },
  "keywords": [
    "AlibabaCloud",
    "SSO",
    "CLI"
  ],
  "author": "Jackson Tian",
  "license": "Apache-2.0",
  "devDependencies": {
    "codecov": "^3.8.2",
    "eslint": "^8.54.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0"
  },
  "files": [
    "bin",
    "lib",
    "commands"
  ],
  "dependencies": {
    "cli-table3": "^0.6.3",
    "httpx": "^2.2.7",
    "inquirer": "^9.2.14",
    "kitx": "^2.1.0",
    "minimist": "^1.2.5",
    "open": "^8.2.0",
    "proxy-agent": "^5.0.0"
  },
  "pkg": {
    "assets": [
      "./node_modules/**/*.js"
    ],
    "targets": [
      "node18-win-x64",
      "node18-win-arm64",
      "node18-linux-x64",
      "node18-linux-arm64",
      "node18-macos-x64",
      "node18-macos-arm64"
    ],
    "outputPath": "dist"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliyun/alibabacloud-sso-cli.git"
  },
  "bugs": {
    "url": "https://github.com/aliyun/alibabacloud-sso-cli/issues"
  },
  "homepage": "https://github.com/aliyun/alibabacloud-sso-cli#readme"
}
